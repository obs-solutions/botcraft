<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="accont_move_postion_number" model="ir.ui.view">
        <field name="name">account.move.view</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page/field[@name='invoice_line_ids']/tree/field[@name='product_id']" position="before">
                <field name="position_number" optional="hide"/>
            </xpath>
        </field>
    </record>

</odoo>
