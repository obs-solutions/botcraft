<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_picking_postion_number" model="ir.ui.view">
        <field name="name">stock.picking.view</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page/field[@name='move_ids_without_package']/tree/field[@name='company_id']" position="before">
                <field name="position_number" optional="hide"/>
            </xpath>
        </field>
    </record>

</odoo>
